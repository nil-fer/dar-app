<tbody>
  <% @categories.each do |category| %>
    <tr>
      <td><%= category.title %></td>
      <td>
        <% category.tag_list.each do |tag| %>
          <%= link_to tag, tagged_path(tag: tag) %>
        <% end %>
      </td>
    </tr>
  <% end %>

  <td><%= link_to 'Back', categories_url %></td>
</tbody>
